{
    "builds": [
      { 
        "src": "app.py", 
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "app.py" }
    ],
    "functions": {
      "app.py": {
        "maxDuration": 30,
        "memory": 512
      }
    },
    "env": {
      "VERCEL": "1"
    }
  }
  